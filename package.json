{
  "name": "email-contact",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://chuihinwai.github.io/email-contact",
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "classnames": "^2.2.6",
    "core-js": "^3.0.1",
    "query-string": "^6.4.2",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-device-detect": "^1.6.2",
    "react-dom": "^16.8.6",
    "react-geolocated": "^2.4.1",
    "react-scripts": "2.1.8",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "use-debounce": "^1.1.2"
  },
  "scripts": {
    "start": "HTTPS=true react-scripts start",
    "start:ci": "REACT_APP_ADDRESS_SERVER_HOST=https://address2contact.herokuapp.com react-scripts start",
    "build": "react-scripts build",
    "test": "npm-run-all test:units",
    "test:ci": "CI=true npm-run-all test:units:junit test:e2e:ci",
    "test:units": "react-scripts test",
    "test:units:junit": "npm run test:units -- --reporters=default --reporters=jest-junit",
    "test:e2e:ci": "start-server-and-test start:ci http://localhost:3000 report",
    "cypress:open": "cypress open",
    "delete:reports": "rm cypress/results/* || true",
    "prereport": "npm run delete:reports",
    "report": "npm run cy:run:ci",
    "cy:run": "cypress run",
    "cy:run:ci": "npm run cy:run -- --config baseUrl=http://localhost:3000 --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/request-promise-native": "^1.0.15",
    "cypress": "^3.2.0",
    "eslint-plugin-cypress": "^2.2.1",
    "gh-pages": "^2.0.1",
    "jest-dom": "^3.1.3",
    "jest-junit": "^6.3.0",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.22.0",
    "mocha-multi-reporters": "^1.1.7",
    "npm-run-all": "^4.1.5",
    "react-testing-library": "^6.1.2",
    "start-server-and-test": "^1.9.0"
  }
}
